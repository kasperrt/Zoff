    <body class="noselect">
        {{{body}}}
        <div class="tap-target" data-activates="castButton">
            <div class="tap-target-content">
                <h5>Chromecast</h5>
                <p>This site supports chromecasting!</p>
            </div>
        </div>
        {{> contact}}
        {{> donate}}
        {{> footer}}
        <textarea class="copy_video_id" style="display:none;"></textarea>
    </body>
